# CoinPay Frontend Environment Variables
# Copy this file to .env.development and update values as needed

# API Configuration
VITE_API_BASE_URL=http://localhost:5100
VITE_API_TIMEOUT=30000

# Application Configuration
VITE_APP_NAME=CoinPay
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_LOGGING=true
VITE_ENABLE_MOCK_API=false

# Circle Configuration (if needed for frontend)
VITE_CIRCLE_APP_ID=

# Environment
VITE_NODE_ENV=development
